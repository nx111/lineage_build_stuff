From 2652d44c55e00426b186712e380b087ff76f5749 Mon Sep 17 00:00:00 2001
From: Michael Bestas <mkbestas@lineageos.org>
Date: Wed, 27 Feb 2019 00:17:33 +0200
Subject: [PATCH] Email: Fix generating id in android namespace

Fixes:
res/layout/preference_mailbox_item.xml:39: warn:
generated id 'android:id/title' for external package 'android'.

Change-Id: I93e613deae5b4b59d834a03f6c1ba828e361d889
---
 res/layout/preference_mailbox_item.xml                    | 2 +-
 src/com/android/email/activity/setup/MailboxSettings.java | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/res/layout/preference_mailbox_item.xml b/res/layout/preference_mailbox_item.xml
index a624d491a..f0271b5c1 100644
--- a/res/layout/preference_mailbox_item.xml
+++ b/res/layout/preference_mailbox_item.xml
@@ -36,7 +36,7 @@
         android:layout_marginEnd="8dip"
         android:layout_gravity="center" />
 
-    <TextView android:id="@+android:id/title"
+    <TextView android:id="@+id/title"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_weight="1"
diff --git a/src/com/android/email/activity/setup/MailboxSettings.java b/src/com/android/email/activity/setup/MailboxSettings.java
index 4b242e4bc..682d17d3a 100644
--- a/src/com/android/email/activity/setup/MailboxSettings.java
+++ b/src/com/android/email/activity/setup/MailboxSettings.java
@@ -295,7 +295,7 @@ public class MailboxSettings extends BasePreferenceActivity {
                 holder = new HeaderViewHolder();
                 holder.spacer = view.findViewById(R.id.spacer);
                 holder.icon = (ImageView) view.findViewById(R.id.icon);
-                holder.title = (TextView) view.findViewById(android.R.id.title);
+                holder.title = (TextView) view.findViewById(R.id.title);
                 holder.checkmark = view.findViewById(R.id.checkmark);
                 view.setTag(holder);
             } else {
-- 
2.17.1


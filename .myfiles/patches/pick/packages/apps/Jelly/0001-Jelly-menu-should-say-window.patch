From d2031d9ee8a95ceecceb902270a0cda0e0026e48 Mon Sep 17 00:00:00 2001
From: syphyr <syphyr@gmail.com>
Date: Mon, 18 Mar 2019 21:26:30 +0100
Subject: [PATCH] Jelly: menu should say window

Jelly does not actually support tabs

Change-Id: Id8c79c0ff8cc9c6bbe3cec432781262fc1efdec9
---
 app/src/main/res/values/strings.xml | 16 ++++++++--------
 1 file changed, 8 insertions(+), 8 deletions(-)

diff --git a/app/src/main/res/values/strings.xml b/app/src/main/res/values/strings.xml
index 6f30738..d97b155 100644
--- a/app/src/main/res/values/strings.xml
+++ b/app/src/main/res/values/strings.xml
@@ -16,10 +16,10 @@
 <resources>
     <string name="app_name">Browser</string>
 
-    <!-- Menu action: create new tab -->
-    <string name="menu_new_tab">New tab</string>
-    <!-- Menu action: create new incognito tab -->
-    <string name="menu_incognito">Incognito tab</string>
+    <!-- Menu action: create new window -->
+    <string name="menu_new_tab">New window</string>
+    <!-- Menu action: create new incognito window -->
+    <string name="menu_incognito">Incognito window</string>
     <!-- Menu action: reload current page -->
     <string name="menu_reload">Reload</string>
     <!-- Menu action: add current page to favorites -->
@@ -36,8 +36,8 @@
     <string name="menu_settings">Settings</string>
     <!-- Menu action: download selected file / image -->
     <string name="menu_download">Download</string>
-    <!-- Menu action: open selected link in a new tab -->
-    <string name="menu_in_new_tab">Open in new tab</string>
+    <!-- Menu action: open selected link in a new window -->
+    <string name="menu_in_new_tab">Open in new window</string>
     <!-- Menu action: enable desktop mode -->
     <string name="menu_desktop_mode">Switch to desktop site</string>
     <!-- Menu action: enable phone mode -->
@@ -178,8 +178,8 @@
     <!-- Auth dialog: login button -->
     <string name="auth_dialog_login">Log in</string>
 
-    <!-- Launcher shortcut: new incognito tab -->
-    <string name="shortcut_new_incognito_tab">New incognito tab</string>
+    <!-- Launcher shortcut: new incognito window -->
+    <string name="shortcut_new_incognito_tab">New incognito window</string>
 
     <!-- Suggestions provider: no suggestions provider -->
     <string name="suggestions_provider_none">None</string>
-- 
2.17.1


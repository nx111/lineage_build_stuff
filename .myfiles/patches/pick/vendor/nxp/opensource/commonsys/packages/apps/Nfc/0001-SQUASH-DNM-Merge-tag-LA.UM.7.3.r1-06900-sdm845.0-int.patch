+    mLmrtEntries[xx].proto_screen_lock = mLmrtEntries[xx].tech_screen_lock =
+        0x00;
+    mLmrtEntries[xx].proto_screen_off = mLmrtEntries[xx].tech_screen_off = 0x00;
+    mLmrtEntries[xx].proto_screen_off_lock =

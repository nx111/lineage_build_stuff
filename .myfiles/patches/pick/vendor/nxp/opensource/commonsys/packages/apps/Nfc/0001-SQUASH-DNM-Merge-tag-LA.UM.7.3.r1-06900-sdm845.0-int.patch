+**
+*******************************************************************************/
+static int nativeNfcMposManage_doMposSetReaderMode(JNIEnv*, jobject, bool on) {
+  tNFA_STATUS status = NFA_STATUS_REJECTED;
+  DLOG_IF(INFO, nfc_debug_enabled) << StringPrintf("%s:enter", __func__);

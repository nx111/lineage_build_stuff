-        {
-            LengthRemaining =
-                (((*TempLength) < PH_FRINFC_MIFARESTD_BYTES_READ)?
-                PH_FRINFC_MIFARESTD_VAL0:
-                (NdefMap->SendRecvBuf[TempLen] -

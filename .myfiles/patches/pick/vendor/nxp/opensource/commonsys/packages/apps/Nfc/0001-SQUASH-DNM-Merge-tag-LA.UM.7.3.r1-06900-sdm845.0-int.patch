+  } else {
+    DLOG_IF(INFO, nfc_debug_enabled) << StringPrintf(
+        "%s: NFA_SendVsCommand failed stat = %d", __func__, stat);
+  }
+  if (type == false) android::mSPIDwpSyncMutex.unlock();

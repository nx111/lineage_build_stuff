-    }
-
-    if( bNdefReq == PH_FRINFC_NDEF_READ_REQ )
-    {
-        if( (Offset != PH_FRINFC_NDEFMAP_SEEK_CUR) && (Offset != PH_FRINFC_NDEFMAP_SEEK_BEGIN) )

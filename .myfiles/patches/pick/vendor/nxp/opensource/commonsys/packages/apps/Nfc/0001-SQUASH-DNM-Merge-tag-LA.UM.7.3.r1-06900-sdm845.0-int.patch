+*******************************************************************************/
+static NFCSTATUS phFriNfc_MfStd_H_ProCon(
+    phFriNfc_sNdefSmtCrdFmt_t* NdefSmtCrdFmt) {
+  NFCSTATUS Result = NFCSTATUS_SUCCESS;
+  uint8_t Buffer[1] = {PH_FRINFC_MFSTD_FMT_NDEF_COMPL};

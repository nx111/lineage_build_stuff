+      SecureElement::getInstance().getSETechnology(ROUTE_LOC_ESE_ID);
+  mTechSupportedByUicc1 = SecureElement::getInstance().getSETechnology(
+      SecureElement::getInstance().EE_HANDLE_0xF4);

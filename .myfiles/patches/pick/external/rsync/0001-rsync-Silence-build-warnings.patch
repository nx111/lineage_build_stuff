From 4a30b87d69b2e97aca221cc71307319e5a5a5492 Mon Sep 17 00:00:00 2001
From: Michael Bestas <mkbestas@lineageos.org>
Date: Mon, 25 Feb 2019 20:19:27 +0200
Subject: [PATCH] rsync: Silence build warnings

Change-Id: I4d2fbf2be25fe8c13dfadd61e83427d1675e2a9f
---
 Android.mk | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/Android.mk b/Android.mk
index a0688c8b..1db868bc 100644
--- a/Android.mk
+++ b/Android.mk
@@ -72,7 +72,9 @@ LOCAL_SRC_FILES := \
 LOCAL_C_INCLUDES := $(LOCAL_PATH)/popt $(LOCAL_PATH)/zlib
 
 LOCAL_CFLAGS := -DHAVE_CONFIG_H
-LOCAL_CFLAGS += -Wno-error
+LOCAL_CFLAGS += \
+	-Wno-incompatible-pointer-types \
+	-Wno-deprecated-declarations
 
 LOCAL_MODULE := rsync
 LOCAL_MODULE_PATH := $(TARGET_OUT_OPTIONAL_EXECUTABLES)
-- 

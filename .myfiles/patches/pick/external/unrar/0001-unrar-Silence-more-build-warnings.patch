From 9b2e2b9f082651133367772117a0845dc346f0b1 Mon Sep 17 00:00:00 2001
From: Michael Bestas <mkbestas@lineageos.org>
Date: Mon, 25 Feb 2019 19:38:32 +0200
Subject: [PATCH] unrar: Silence more build warnings

Change-Id: I8a29a99b6542d1e88d70b7bb80e94b28e23f65fb
---
 Android.mk | 11 +++++++++--
 1 file changed, 9 insertions(+), 2 deletions(-)

diff --git a/Android.mk b/Android.mk
index d9d60ac..4782f8f 100644
--- a/Android.mk
+++ b/Android.mk
@@ -16,7 +16,15 @@ LOCAL_C_INCLUDES := src
 LOCAL_CFLAGS := -O2 -Wall -DUNRAR -D__BIONIC__ \
                 -D_LARGEFILE_SOURCE -DNOVOLUME -DRAR_NOCRYPT
 
-LOCAL_CFLAGS += -Wno-error -Wno-unused-variable -Wno-unused-parameter -Wno-sign-compare -Wno-logical-op-parentheses -Wno-dangling-else
+LOCAL_CFLAGS += \
+    -Wno-dangling-else \
+    -Wno-logical-op-parentheses \
+    -Wno-missing-braces \
+    -Wno-sign-compare \
+    -Wno-switch \
+    -Wno-unused-function \
+    -Wno-unused-parameter \
+    -Wno-unused-variable
 
 LOCAL_ARM_MODE := arm
 
@@ -25,4 +33,3 @@ LOCAL_MODULE_TAGS := optional
 LOCAL_MODULE_PATH := $(TARGET_OUT_OPTIONAL_EXECUTABLES)
 
 include $(BUILD_EXECUTABLE)
-
-- 
2.17.1


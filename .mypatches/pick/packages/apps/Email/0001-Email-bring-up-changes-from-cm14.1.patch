+                i.putExtra(EXTRA_MESSAGE_INFO,
+                        intent.getIntExtra(EXTRA_MESSAGE_INFO, 0));
+                context.startService(i);
+            } else {
+                Log.i(TAG, "READ MESSAGE POP3 NOT Implemented");
